{
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Community Material Theme Darker High Contrast",
    "terminal.integrated.fontFamily": "Inconsolata Nerd Font",
    "editor.fontFamily": "FuraCode Nerd Font, Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "material-icon-theme.folders.associations": {
		"module": "font",
		"component": "components",
		"doc": "markdown",
		"animation": "delta",
		"guard": "ngrx-effects",
		"directive": "ngrx-actions",
		"interceptor": "middleware",
		"validator": "coverage",
		"e2e": "test",
		"target": "dist",
		"rust": "delta",
		"benches": "benchmark"
	},
	"material-icon-theme.files.associations": {
		"*.animation.ts": "purescript",
		".mocharc": "mocha"
	},
	"git.confirmSync": false,
	"git.autofetch": true,
	"git.enableSmartCommit": true,
	"editor.mouseWheelZoom": true,
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"explorer.confirmDragAndDrop": false,
	"terminal.integrated.shell.linux": "/usr/bin/tcsh",
}