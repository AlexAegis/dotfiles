# Host specific config should go into config.local
Include config.local

# Example setup using https://sdf.org/
Host sdf
    HostName tty.sdf.org
    User alexaegis
    Port 22
    IdentityFile ~/.ssh/id_rsa

Host gh
    HostName github.com
    User alexaegis
    Port 22
    IdentityFile ~/.ssh/id_rsa
