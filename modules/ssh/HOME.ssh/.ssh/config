# Allow other modules to plug in their ssh configs
Include config.d/*

Host sdf
    HostName tty.sdf.org
    User alexaegis
    Port 22
    IdentityFile ~/.ssh/id_rsa

Host github.com
    HostName github.com
    User git
    Port 22
    IdentityFile ~/.ssh/gh_rsa

Host *
	IgnoreUnknown AddKeysToAgent,UseKeychain
	AddKeysToAgent yes
	UseKeychain yes
    IdentitiesOnly yes
